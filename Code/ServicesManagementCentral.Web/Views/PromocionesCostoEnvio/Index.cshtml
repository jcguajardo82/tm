
@model IEnumerable<ServicesManagement.Web.Models.CarrierModel>

@{

    Layout = "~/Views/Shared/_Layout.cshtml";

    ViewBag.Title = "Promociones $ Envío";

}


<link href="view-source:http://webapplayers.com/inspinia_admin-v2.8/sweetalert.html">

<div class="row">
    <div class="col-4"></div>
    <div class="col-4"><h2>Promociones $ Envío</h2></div>
    <div class="col-4"></div>
</div>
<div class="ibox-content p-xl" style="padding-top: 0px;padding-bottom: 0px;">
    <br />
    <div class="row">
        <div class="col-12">
            <table id="tblConsulta" class="table table-striped table-bordered table-hover">
                <thead>
                    @*<thead class="bg-primary">*@
                    <tr>
                        <th>PostalCodeOrig</th>
                        <th>PostalCodeDestino</th>
                        <th>CiudadOrig</th>
                        <th>CiudadDest</th>
                        <th>EdoOrig</th>
                        <th>EdoDest</th>
                        <th>SupplierName</th>
                        <th>NombreTransportista</th>
                        <th>Cve_CategSAP</th>
                        <th>Desc_CategSAP</th>
                        <th>Cve_GciaCategSAP</th>
                        <th>Desc_GciaCategSAP</th>
                        <th>Material_MATNR</th>
                        <th>Id_Num_CodBarra</th>
                        <th>Nombre_SKU</th>
                        <th>PesoMinimo</th>
                        <th>PesoMaximo</th>
                        <th>MesesSinIntereses</th>
                        <th>ComprasMayor</th>
                        <th>ComprasMenor</th>
                        <th>FechaInicioPromo</th>
                        <th>HoraInicioPromo</th>
                        <th>FechaFinPromo</th>
                        <th>HoraFinPromo</th>
                        <th>CostoEspecial</th>
                        <th>TarifaDesc</th>
                        <th>UsuarioCreacion</th>
                        <th>Activo</th>
                        <th name="accion">Action</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
<input type="hidden" id="hddId" />
<div id="modal-form" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title">Promociones $ Envío</h2>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="frmCreateServicios">
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Almacén</label>
                                <select class="form-control" id="ddlAlmacen">
                                    <option selected="selected" value="0">--Tipo de Almacen--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Proveedor</label>
                                <select class="form-control" id="ddlProveedor">
                                    <option selected="selected" value="0">--Proveedor--</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Transportista</label>
                                <select class="form-control" id="ddlTransportista">
                                    <option selected="selected" value="0">--Transportista--</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Catalogo</label>
                                <select class="form-control" id="ddlCatalogo">
                                    <option selected="selected" value="0">--SELECCIONE--</option>
                                    <option value="1">Super</option>
                                    <option value="2">Mercancías Generales</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Formato Tienda</label>
                                <select class="form-control" id="ddlFormatoTienda">
                                    <option selected="selected" value="0">--Proveedor--</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Envio</label>
                                <select class="form-control" id="ddlTipoEnvio">
                                    <option selected="selected" value="0">--Transportista--</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Servicio</label>
                                <select class="form-control" id="ddlTipoServicio">
                                    <option selected="selected" value="0">--Tipo de Servicio--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo Logistica</label>
                                <select class="form-control" id="ddlTipoLogistica">
                                    <option selected="selected" value="0">--Tipo Logistica--</option>

                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>C.P. Origen</label>
                                <input type="number" id="txtCpOrigen" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>C.P. Destino</label>
                                <input type="number" id="txtCpDestino" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Ciudad Origen</label>
                                <input type="text" id="txtCdOrigen" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Ciudad Destino</label>
                                <input type="text" id="txtCdDestino" class="form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Estado Origen</label>
                                <input type="text" id="txtOrigen" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Estado Destino</label>
                                <input type="text" id="txtDestino" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Material</label>
                                <input type="text" id="txtNoMaterial" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>SKU</label>
                                <input type="text" id="txtSKU" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de EAN</label>
                                <input type="text" id="txtCodBar" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Peso Minimo</label>
                                <input type="number" oninput='input(this)' id="txtPesoMin" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Peso Maximo</label>
                                <input type="number" oninput='input(this)' id="txtPesoMax" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Meses Sin Intereses</label>
                                <input type="number" id="txtMeses" class="form-control" />
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Fecha inical de tarifa especial</label>
                                @*<input type="date" id="txtFecIniTarifa" class="form-control" />*@
                                <div class="input-group date" id="datetimepicker1">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input class="form-control datepicker" id="txtFecIniTarifa" name="txtFecIniTarifa"
                                           type="text" value="">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Hora inical de tarifa especial</label>
                                <input type="time" id="txtHoraIniTarifa" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Fecha final de tarifa especial</label>
                                @*<input type="date" id="txtFecFinTarifa" class="form-control" />*@
                                <div class="input-group date" id="datetimepicker1">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input class="form-control datepicker" id="txtFecFinTarifa" name="txtFecFinTarifa"
                                           type="text" value="">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Hora final de tarifa especial</label>
                                <input type="time" id="txtHoraFinTarifa" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Categoria (Categoría SAP)</label>
                                <input type="text" id="txtNoCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Nombre de Categoria (Categoría SAP)</label>
                                <input type="text" id="txtNomCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Subcategoria (Categoría SAP)</label>
                                <input type="text" id="txtNoSubCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Nombre de Subcategoria (Categoría SAP)</label>
                                <input type="text" id="txtNomSubCatSap" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>
                                    Costo especial ($)
                                </label>
                                <input type="number" oninput='input(this)' id="txtCostoEsp" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Aplica tarifa descuento  (%) </label>
                                <input type="number" oninput='input(this)' id="txtTarifaDesc" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>$ Compras mayor a </label>
                                <input type="number" oninput='input(this)' id="txtComprasMayor" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>$ Compras menor a</label>
                                <input type="number" oninput='input(this)' id="txtComprasMenor" class="form-control" />
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-xs-4 col-md-4">
                            <div class="form-group">
                                <label>Estatus</label>
                                <select class="form-control" id="id_estatus" name="id_estatus">
                                    @*<option selected="selected" value="1">--SELECCIONA--</option>*@
                                    <option selected="selected" value="1">ACTIVO</option>
                                    <option value="0">INACTIVO</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                <button type="button" onclick="Add()" class="btn btn-primary">Guardar</button>
            </div>
        </div>
    </div>
</div>


@section scripts {


    <script>
        $(function () {
            $('#datetimepicker1').datetimepicker({
                locale: 'es'
                , format: "DD/MM/yyyy"
            });
            $('#datetimepicker2').datetimepicker({
                locale: 'es'
                , format: "DD/MM/yyyy"
            });
            $('#datetimepicker3').datetimepicker({
                locale: 'es'
                , format: "DD/MM/yyyy"
            });
        });
        $(document).ready(function () {

            $('#tblConsulta thead tr').clone(true).appendTo('#tblConsulta thead');
            $('#tblConsulta thead tr:eq(0) th').each(function (i) {
                if ($(this).attr('name') != "accion") {
                    var title = $(this).text();
                    $(this).html('<input type="text" class="form form-control input-sm" placeholder="' + title + '" />');

                    $('input', this).on('keyup change', function () {
                        if (table.column(i).search() !== this.value) {
                            table
                                .column(i)
                                .search(this.value)
                                .draw();
                        }
                    });
                } else {
                    $(this).empty();
                }
            });

            table = $('#tblConsulta').DataTable(
                {
                    "scrollX": true,
                    language:
                    {
                        "sProcessing": "Procesando...",
                        "sLengthMenu": "Mostrar _MENU_ registros",
                        "sZeroRecords": "No se encontraron resultados",
                        "sEmptyTable": "Ningún dato disponible en esta tabla",
                        "sInfo": "Mostrando registros del _START_ al _END_ de un total de _TOTAL_ registros",
                        "sInfoEmpty": "Mostrando registros del 0 al 0 de un total de 0 registros",
                        "sInfoFiltered": "(filtrado de un total de _MAX_ registros)",
                        "sInfoPostFix": "",
                        "sSearch": "Buscar:",
                        "sUrl": "",
                        "sInfoThousands": ",",
                        "sLoadingRecords": "Cargando...",
                        "oPaginate": {
                            "sFirst": "Primero",
                            "sLast": "Último",
                            "sNext": "Siguiente",
                            "sPrevious": "Anterior"

                        },
                        "oAria": {
                            "sSortAscending": ": Activar para ordenar la columna de manera ascendente",
                            "sSortDescending": ": Activar para ordenar la columna de manera descendente"

                        },
                        "buttons": {
                            "print": "Imprimir",
                            "pageLength": 'Mostrar %d Registros'

                        }
                    },
                    dom: 'Bfrtip',
                    lengthMenu: [
                        [10, 25, 50, -1],
                        ['10 Registros', '25 Registros', '50 Registros', 'Mostrar Todos']
                    ],
                    buttons: [
                        { extend: 'pageLength', className: 'btn-primary' },
                        { extend: 'excel', className: 'btn-primary' },
                        {
                            text: 'Alta',
                            action: function (e, dt, node, config) {

                                $("#hddId").val('0');
                                $("#modal-form").modal('show');

                            }
                            , className: 'btn-primary'
                        }
                    ]
                    //,"fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
                    //    if (aData[1] == "SETC") {
                    //        $('td', nRow).css('background-color', '#B6BF00');
                    //    } else if (aData[2] == "4") {
                    //        $('td', nRow).css('background-color', 'Orange');
                    //    }
                    //}
                }
            );

            GetConsulta();
            IniciarComboTipoAlmacen();
            CmboTipoEnvio();
            CmboTipoServicio();
            CmboTipoLogistica();
            CmboTransportista();
            CmboFormatoTienda();
        });

        function GetConsulta() {
            //var _Id_Num_UN = $("#hddId").val();
            //debugger;
            $.ajax({
                async: false,
                type: "GET",
                url: '@Url.Action("GetPromociones", "PromocionesCostoEnvio")',
                data: {
                    //  Id_Num_UN: _Id_Num_UN
                },
                datatype: "html",
                success: function (json) {
                    console.log(json);

                    debugger;
                    if (json.Success) {


                        var myTable = $('#tblConsulta').DataTable();
                        myTable
                            .clear()
                            .draw();
                        for (var i = 0; i < json.resp.length; i++) {


                            myTable.row.add([
                                json.resp[i].PostalCodeOrig,
                                json.resp[i].PostalCodeDestino,
                                json.resp[i].CiudadOrig,
                                json.resp[i].CiudadDest,
                                json.resp[i].EdoOrig,
                                json.resp[i].EdoDest,
                                json.resp[i].SupplierName,
                                json.resp[i].NombreTransportista,
                                json.resp[i].Cve_CategSAP,
                                json.resp[i].Desc_CategSAP,
                                json.resp[i].Cve_GciaCategSAP,
                                json.resp[i].Desc_GciaCategSAP,
                                json.resp[i].Material_MATNR,
                                json.resp[i].Id_Num_CodBarra,
                                json.resp[i].Nombre_SKU,
                                json.resp[i].PesoMinimo,
                                json.resp[i].PesoMaximo,
                                json.resp[i].MesesSinIntereses,
                                json.resp[i].ComprasMayor,
                                json.resp[i].ComprasMenor,
                                json.resp[i].FechaInicioPromo,
                                json.resp[i].HoraInicioPromo,
                                json.resp[i].FechaFinPromo,
                                json.resp[i].HoraFinPromo,
                                json.resp[i].CostoEspecial,
                                json.resp[i].TarifaDesc,
                                json.resp[i].UsuarioCreacion,
                                json.resp[i].BitActivo,
                                "<button onclick=\"GetById(" + json.resp[i].cnscPromo + ")\" type=\"button\" class=\"btn btn-white btn-circle\"> <i class=\"fa fa-edit\"></i></button><button type=\"button\" onclick=\"Delete(" + json.resp[i].cnscPromo + ")\" class=\"btn btn-warning btn-circle\"> <i class=\"fa fa-minus\"></i></button>"
                            ]).draw();
                        }
                    }
                }
                , error: function (jqXHR, textStatus, errorThrown) {
                    bootbox.alert(errorThrown);
                }
            });
        }

        function ActionUpd() {
            debugger;
            if (document.getElementById("flag_edit").value == "1") {
                Edit();
            }
            else {
                Add();
            }
        }

        function Add() {
        /*   var _IdTipoCatalogo = $("#ddlTipoCatalogo").val();*/
            var _cnscPromo = $("#hddId").val();
            var _IdOwner = $("#ddlAlmacen").val();
            var _IdTipoCatalogo = $("#ddlCatalogo").val();
            var _IdSupplierWH = $("#ddlProveedor").val();
            var _SupplierName= $("#ddlProveedor :selected").text();
            var _IdTransportista = $("#ddlTransportista").val();
            var _NombreTransportista = $("#ddlTransportista :selected").text();
            var _IdFormatoTienda = $("#ddlFormatoTienda").val();
            var _IdTipoEnvio = $("#ddlTipoEnvio").val();
            var _IdTipoServicio = $("#ddlTipoServicio").val();
            var _IdTipoLogistica = $("#ddlTipoLogistica").val();

            if (_IdTipoCatalogo == "0") {
                $("#ddlCatalogo").focus();
                return;
            }
            if (_IdOwner == "0") {
                $("#ddlAlmacen").focus();
                return;
            }
            if (_IdSupplierWH == "0") {
                $("#ddlProveedor").focus();
                return;
            }
            if (_IdTransportista == "0") {
                $("#ddlTransportista").focus();
                return;
            }
            if (_IdFormatoTienda == "0") {
                $("#ddlFormatoTienda").focus();
                return;
            }
            if (_IdTipoEnvio == "0") {
                $("#ddlTipoEnvio").focus();
                return;
            }
            if (_IdTipoServicio == "0") {
                $("#ddlTipoServicio").focus();
                return;
            }
            if (_IdTipoLogistica == "0") {
                $("#ddlTipoLogistica").focus();
                return;
            }


            _BitActivo = $("#id_estatus").val();

            _PostalCodeOrig = $("#txtCpOrigen").val();
            _PostalCodeDestino = $("#txtCpDestino").val();
            _CiudadOrig = $("#txtCdOrigen").val();
            _CiudadDest = $("#txtCdDestino").val();
            _EdoOrig = $("#txtOrigen").val();
            _EdoDest = $("#txtDestino").val();
            _Cve_CategSAP = $("#txtNoCatSap").val();
            _Desc_CategSAP = $("#txtNomCatSap").val();
            _Cve_GciaCategSAP = $("#txtNoSubCatSap").val();
            _Desc_GciaCategSAP = $("#txtNomSubCatSap").val();
            _Material_MATNR = $("#txtNoMaterial").val();
            _Id_Num_CodBarra = $("#txtCodBar").val();
            _nombre_SKU = $("#txtSKU").val();
            _PesoMinimo = $("#txtPesoMin").val();
            _PesoMaximo = $("#txtPesoMax").val();
            _MesesSinIntereses = $("#txtMeses").val();
            _ComprasMayor = $("#txtComprasMayor").val();
            _ComprasMenor = $("#txtComprasMenor").val();
            _FechaInicioPromo = $("#txtFecIniTarifa").val();
            _HoraInicioPromo = $("#txtHoraIniTarifa").val();
            _FechaFinPromo = $("#txtFecFinTarifa").val();
            _HoraFinPromo = $("#txtHoraFinTarifa").val();
            _CostoEspecial = $("#txtCostoEsp").val();
            _TarifaDesc = $("#txtTarifaDesc").val();


            $.ajax({
                async: false,
                type: "GET",
                url: '@Url.Action("AddPromociones", "PromocionesCostoEnvio")',
                data: {
                    cnscPromo: _cnscPromo
                    ,IdTipoCatalogo: _IdTipoCatalogo
                    , IdOwner: _IdOwner
                    , IdFormatoTienda: _IdFormatoTienda
                    , PostalCodeOrig: _PostalCodeOrig
                    , PostalCodeDestino: _PostalCodeDestino
                    , CiudadOrig: _CiudadOrig
                    , CiudadDest: _CiudadDest
                    , EdoOrig: _EdoOrig
                    , EdoDest: _EdoDest
                    , IdSupplierWH: _IdSupplierWH
                    , SupplierName: _SupplierName
                    , IdTransportista: _IdTransportista
                    , NombreTransportista: _NombreTransportista
                    , IdTipoEnvio: _IdTipoEnvio
                    , IdTipoServicio: _IdTipoServicio
                    , Cve_CategSAP: _Cve_CategSAP
                    , Desc_CategSAP: _Desc_CategSAP
                    , Cve_GciaCategSAP: _Cve_GciaCategSAP
                    , Desc_GciaCategSAP: _Desc_GciaCategSAP
                    , Material_MATNR: _Material_MATNR
                    , Id_Num_CodBarra: _Id_Num_CodBarra
                    , nombre_SKU: _nombre_SKU
