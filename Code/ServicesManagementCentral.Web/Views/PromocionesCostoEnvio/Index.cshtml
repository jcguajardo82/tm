
@model IEnumerable<ServicesManagement.Web.Models.CarrierModel>

@{

    Layout = "~/Views/Shared/_Layout.cshtml";

    ViewBag.Title = "Promociones Costo de Envío ";

}

<style>
    div.scroll_horizontal {
        width: 100%;
        overflow: auto;
    }
</style>

<link href="view-source:http://webapplayers.com/inspinia_admin-v2.8/sweetalert.html">

<div class="row">
    <div class="col-4"></div>
    <div class="col-4"><h2>Promociones Costo de Envío</h2></div>
    <div class="col-4"></div>
</div>
<div class="ibox-content p-xl" style="padding-top: 0px;padding-bottom: 0px;">
    <br />
    <div class="row scroll_horizontal">
        <div class="col-12">
            <table id="tblConsulta" class="table table-striped table-bordered table-hover">
                <thead>
                    @*<thead class="bg-primary">*@
                    <tr>
                        <th>Tipo de <br />  Envio</th>
                        <th>Tipo de <br />  Servicio</th>
                        <th>Tipo de <br />  Catalogo</th>
                        <th>Tipo de <br />  Formato</th>
                        <th>Codigo Postal <br />  Origen</th>
                        <th>Codigo Postal <br />  Destino</th>
                        <th>Ciudad <br /> Origen</th>
                        <th>Ciudad <br /> Destino</th>
                        <th>Estado <br /> Origen</th>
                        <th>Estado <br /> Destino</th>
                        <th>Proveedor</th>
                        <th>Transportista</th>
                        <th>Id Categoria <br /> (SAP)</th>
                        <th>Categoria <br /> (SAP)</th>
                        <th>Id SubCategoria <br /> (SAP)</th>
                        <th>SubCategoria <br /> (SAP)</th>
                        <th>Numero de Material</th>
                        <th>EAN</th>
                        <th>Nombre SKU</th>
                        <th>Peso Mínimo</th>
                        <th>Peso Máximo</th>
                        <th>Orden de Ejecución</th>
                        <th>Meses sin <br /> Intereses</th>
                        <th>Compras <br /> Mayores a </th>
                        <th>Compras <br /> Menores a </th>
                        <th>Fecha inical de <br /> tarifa especial</th>
                        <th>Hora inical de <br /> tarifa especial</th>
                        <th>Fecha final de <br /> tarifa especial</th>
                        <th>Hora final de <br /> tarifa especial</th>
                        <th>Costo <br /> especial ($)</th>
                        <th>Aplica tarifa <br /> descuento (%)</th>
                        <th>Usuario de <br /> ultima actualización</th>
                        <th>Fecha de <br /> ultima actualización</th>
                        <th>Hora de <br /> ultima actualización</th>
                        <th>Activo</th>
                        <th name="accion">Action</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>


</div>
<input type="hidden" id="hddId" />
<div id="modal-form" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title">Promociones $ Envío</h2>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="frmCreateServicios">
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Almacén</label>
                                <select class="form-control" id="ddlAlmacen">
                                    <option selected="selected" value="0">--Tipo de Almacen--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Proveedor</label>
                                <select class="form-control" id="ddlProveedor">
                                    <option selected="selected" value="0">--Proveedor--</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Transportista</label>
                                <select class="form-control" id="ddlTransportista">
                                    <option selected="selected" value="0">--Transportista--</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Catalogo</label>
                                <select class="form-control" id="ddlCatalogo">
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Formato Tienda</label>
                                <select class="form-control" id="ddlFormatoTienda">
                                    <option selected="selected" value="0">--Proveedor--</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Envio</label>
                                <select class="form-control" id="ddlTipoEnvio">
                                    <option selected="selected" value="0">--Transportista--</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo de Servicio</label>
                                <select class="form-control" id="ddlTipoServicio">
                                    <option selected="selected" value="0">--Tipo de Servicio--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4 col-md-4 col-lg-4">
                            <div class="form-group">
                                <label>Tipo Logistica</label>
                                <select class="form-control" id="ddlTipoLogistica">
                                    <option selected="selected" value="0">--Tipo Logistica--</option>

                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>C.P. Origen</label>
                                <input type="number" id="txtCpOrigen" onkeypress="return event.charCode >= 48" min="0" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>C.P. Destino</label>
                                <input type="number" id="txtCpDestino" onkeypress="return event.charCode >= 48" min="0" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Ciudad Origen</label>
                                @*<input type="text" id="txtCdOrigen" class="form-control" />*@
                                <select id="ddlCdOrigen" class="form-control">
                                    <option value="0">--SELECCIONE--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Ciudad Destino</label>
                                @*<input type="text" id="txtCdDestino" class="form-control" />*@
                                <select id="ddlCdDestino" class="form-control">
                                    <option value="0">--SELECCIONE--</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Estado Origen</label>
                                @*<input type="text" id="txtOrigen" class="form-control" />*@
                                <select id="ddlEdoOrigen" class="form-control"></select>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Estado Destino</label>
                                @*<input type="text" id="txtDestino" class="form-control" />*@
                                <select id="ddlEdoDestino" class="form-control"></select>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Material</label>
                                <input type="number" onkeypress="return event.charCode >= 48" min="0" id="txtNoMaterial" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Nombre SKU</label>
                                <input type="text" readonly id="txtSKU" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de EAN</label>
                                <input type="number" min="0" onkeypress="return event.charCode >= 48" id="txtCodBar" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Peso Minimo</label>
                                <input type="number" onkeypress="return event.charCode >= 48" min="0" oninput='input(this)' id="txtPesoMin" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Peso Maximo</label>
                                <input type="number" onkeypress="return event.charCode >= 48" min="0" oninput='input(this)' id="txtPesoMax" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Meses Sin Intereses</label>
                                <input type="number" onkeypress="return event.charCode >= 48" min="0" id="txtMeses" class="form-control" />
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Fecha inical de tarifa especial</label>
                                @*<input type="date" id="txtFecIniTarifa" class="form-control" />*@
                                <div class="input-group date" id="datetimepicker1">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input class="form-control datepicker" id="txtFecIniTarifa" name="txtFecIniTarifa"
                                           type="text" value="">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Hora inical de tarifa especial</label>
                                <input type="time" id="txtHoraIniTarifa" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Fecha final de tarifa especial</label>
                                @*<input type="date" id="txtFecFinTarifa" class="form-control" />*@
                                <div class="input-group date" id="datetimepicker2">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input class="form-control datepicker" id="txtFecFinTarifa" name="txtFecFinTarifa"
                                           type="text" value="">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Hora final de tarifa especial</label>
                                <input type="time" id="txtHoraFinTarifa" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Categoria (Categoría SAP)</label>
                                <input type="text" id="txtNoCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Nombre de Categoria (Categoría SAP)</label>
                                <input type="text" id="txtNomCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Número de Subcategoria (Categoría SAP)</label>
                                <input type="text" id="txtNoSubCatSap" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Nombre de Subcategoria (Categoría SAP)</label>
                                <input type="text" id="txtNomSubCatSap" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>
                                    Costo especial ($)
                                </label>
                                <input type="number" oninput='input(this)' onkeypress="return event.charCode >= 48" min="0" id="txtCostoEsp" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Aplica tarifa descuento  (%) </label>
                                <input type="number" oninput='input(this)' onkeypress="return event.charCode >= 48" min="0" id="txtTarifaDesc" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>$ Compras mayor a </label>
                                <input type="number" oninput='input(this)' onkeypress="return event.charCode >= 48" min="0" id="txtComprasMayor" class="form-control" />
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>$ Compras menor a</label>
                                <input type="number" oninput='input(this)' onkeypress="return event.charCode >= 48" min="0" id="txtComprasMenor" class="form-control" />
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-xs-4 col-md-4">
                            <div class="form-group">
                                <label>Estatus</label>
                                <select class="form-control" id="id_estatus" name="id_estatus">
                                    @*<option selected="selected" value="1">--SELECCIONA--</option>*@
                                    <option selected="selected" value="1">ACTIVO</option>
                                    <option value="0">INACTIVO</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <div class="form-group">
                                <label>Orden Ejecución </label>
                                <input type="number" step="1" onkeypress="return event.charCode >= 48 && event.charCode <= 57" min="1" id="txtOrden" class="form-control" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                <button type="button" onclick="Add()" class="btn btn-primary">Guardar</button>
            </div>
        </div>
    </div>
</div>


@section scripts {


    <script>
        $(function () {
            $('#datetimepicker1').datetimepicker({
                locale: 'es'
                , format: "yyyy/MM/DD"
            });
            $('#datetimepicker2').datetimepicker({
                locale: 'es'
                , format: "yyyy/MM/DD"
            });
            $('#datetimepicker3').datetimepicker({
                locale: 'es'
                , format: "yyyy/MM/DD"
            });
        });
        $(document).ready(function () {

        $('#tblConsulta thead tr').clone(true).appendTo('#tblConsulta thead');
        $('#tblConsulta thead tr:eq(0) th').each(function (i) {
            if ($(this).attr('name') != "accion") {
                var title = $(this).text();
                $(this).html("<input type='search' class='form-control form-control-sm' placeholder='" + title + "' id='filtro"+i+"'  />");

                $('input', this).on('keyup change', function () {
                    debugger;
                    if (table.column(i).search() !== this.value) {
                        debugger;
                        table
                            .column(i)
                            .search(this.value)
                            .draw();
                    }
                });

                $('input', this).on("input", function () {
                    table
                        .column(i)
                        .search(this.value)
                        .draw();
                });
            } else {
                $(this).empty();
            }
        });

        table = $('#tblConsulta').DataTable({
            "processing": true,
            "serverSide": true,
            "ajax": {
                "url": "@Url.Content("~/PromocionesCostoEnvio/GetPromociones")",
     
                "type": "POST",
                "datatype":"json"
            },
            "pageLength": 10,
            "filter": true,
            "responsivePriority": 1,
            "data": null,
            "columns": [
                { "data": "Desc_TipoEnvio", "name": "Desc_TipoEnvio", "autoWidth": true },
                { "data": "Desc_TipoServicio", "name": "Desc_TipoServicio", "autoWidth": true },
                { "data": "TipoCatalogo", "name": "TipoCatalogo", "autoWidth": true },
                { "data": "Desc_TipoFormato", "name": "Desc_TipoFormato", "autoWidth": true },
                { "data": "PostalCodeOrig", "name": "PostalCodeOrig", "autoWidth": true },
                { "data": "PostalCodeDestino", "name": "PostalCodeDestino", "autoWidth": true },
                { "data": "CiudadOrig", "name": "CiudadOrig", "autoWidth": true },
                { "data": "CiudadDest", "name": "CiudadDest", "autoWidth": true },
                { "data": "EdoOrig", "name": "EdoOrig", "autoWidth": true },
                { "data": "EdoDest", "name": "EdoDest", "autoWidth": true },
                { "data": "SupplierName", "name": "SupplierName", "autoWidth": true },
                { "data": "NombreTransportista", "name": "NombreTransportista", "autoWidth": true },
                { "data": "Cve_CategSAP", "name": "Cve_CategSAP", "autoWidth": true },
                { "data": "Desc_CategSAP", "name": "Desc_CategSAP", "autoWidth": true },
                { "data": "Cve_GciaCategSAP", "name": "Cve_GciaCategSAP", "autoWidth": true },
                { "data": "Desc_GciaCategSAP", "name": "Desc_GciaCategSAP", "autoWidth": true },
                { "data": "Material_MATNR", "name": "Material_MATNR", "autoWidth": true },
                { "data": "Id_Num_CodBarra", "name": "Id_Num_CodBarra", "autoWidth": true },
                { "data": "Nombre_SKU", "name": "Nombre_SKU", "autoWidth": true },
                { "data": "PesoMinimo", "name": "PesoMinimo", "autoWidth": true },
                { "data": "PesoMaximo", "name": "PesoMaximo", "autoWidth": true },
                { "data": "Orden", "name": "Orden", "autoWidth": true },
                { "data": "MesesSinIntereses", "name": "MesesSinIntereses", "autoWidth": true },
                { "data": "ComprasMayor", "name": "ComprasMayor", "autoWidth": true },
                { "data": "ComprasMenor", "name": "ComprasMenor", "autoWidth": true },
                { "data": "FechaInicioPromo", "name": "FechaInicioPromo", "autoWidth": true },
                { "data": "HoraInicioPromo", "name": "HoraInicioPromo", "autoWidth": true },
                { "data": "FechaFinPromo", "name": "FechaFinPromo", "autoWidth": true },
                { "data": "HoraFinPromo", "name": "HoraFinPromo", "autoWidth": true },
                { "data": "CostoEspecial", "name": "CostoEspecial", "autoWidth": true },
                { "data": "TarifaDesc", "name": "TarifaDesc", "autoWidth": true },
                { "data": "UsuarioCreacion", "name": "UsuarioCreacion", "autoWidth": true },
                { "data": "FechaUltModif", "name": "FechaUltModif", "autoWidth": true },
                { "data": "HoraUltModif", "name": "HoraUltModif", "autoWidth": true },
                {
                    //adds td row for button
                    data: null,
                    render: function (data, type, row) {
                        var a = "Inactivo";
                        if (data.BitActivo == "1") {
                            a = "Activo";
                        }
                        return a;


                    }
                },
                {
                    //adds td row for button
                    data: null,
                    render: function (data, type, row) {
                        return "<button title='Editar' onclick='GetById(" + data.cnscPromo + ")' type='button' class='btn btn-white btn-circle'> <i class='fa fa-edit'></i></button><button title='Eliminar' type='button' onclick='Delete(" + data.cnscPromo + ")' class='btn btn-warning btn-circle'> <i class='fa fa-minus'></i></button>";


                    }
                }


            ]
            , language:
            {
                "sProcessing": "Procesando...",
                "sLengthMenu": "Mostrar _MENU_ registros",
                "sZeroRecords": "No se encontraron resultados",
                "sEmptyTable": "Ningún dato disponible en esta tabla",
                "sInfo": "Mostrando registros del _START_ al _END_ de un total de _TOTAL_ registros",
                "sInfoEmpty": "Mostrando registros del 0 al 0 de un total de 0 registros",
                "sInfoFiltered": "(filtrado de un total de _MAX_ registros)",
                "sInfoPostFix": "",
                "sSearch": "Buscar:",
                "sUrl": "",
                "sInfoThousands": ",",
                "sLoadingRecords": "Cargando...",
                "oPaginate": {
                    "sFirst": "Primero",
                    "sLast": "Último",
                    "sNext": "Siguiente",
                    "sPrevious": "Anterior"

                },
                "oAria": {
                    "sSortAscending": ": Activar para ordenar la columna de manera ascendente",
                    "sSortDescending": ": Activar para ordenar la columna de manera descendente"

                },
                "buttons": {
                    "print": "Imprimir",
                    "pageLength": 'Mostrar %d Registros'

                }
            },
            dom: 'Bfrtip',
            lengthMenu: [
                [10, 25, 50, -1],
                ['10 Registros', '25 Registros', '50 Registros', 'Mostrar Todos']
            ],
            buttons: [
                { extend: 'pageLength', className: 'btn-primary' },
                {
                    text: 'Excel General',
                    action: function (e, dt, node, config) {
                        Excel();
                    }
                    , className: 'btn-primary'
                },
                {
                    text: 'Alta',
                    action: function (e, dt, node, config) {

                        $("#hddId").val('0');


                        IniciarControles(1);

                        $("#modal-form").modal('show');

                    }
                    , className: 'btn-primary'
                }

            ],
            fixedHeader: {
                header: true,
                footer: true
            },
            "scrollX": true
            , "scrollY": "500px"
            , "scrollCollapse": true
        });

            //GetConsulta();
            IniciarComboTipoAlmacen();
            CmboTipoEnvio();
            CmboTipoServicio();
            CmboTipoLogistica();
            CmboTransportista();
            CmboTipoCatalogo();
            CmboFormatoTienda();
            CboEdos();
        });

        $('#txtNoMaterial').on('keypress', function (e) {
            debugger;
            if (e.which === 13) {

                GetArt(1);
            }
        });

        $('#txtCodBar').on('keypress', function (e) {
            debugger;
            if (e.which === 13) {

                GetArt(2);
            }
        });

        function GetArt(op) {
            _Material_MATNR = $('#txtNoMaterial').val();
            _Id_Num_CodBarra = $('#txtCodBar').val();

                 $.ajax({
                    async: false,
                    type: "GET",
                    url: '@Url.Action("GetArt", "PromocionesCostoEnvio")',
                    data: {
                        Material_MATNR: _Material_MATNR
                        , Id_Num_CodBarra: _Id_Num_CodBarra
                    },
                    datatype: "html",
                    success: function (json) {
                        if (json.Success) {
