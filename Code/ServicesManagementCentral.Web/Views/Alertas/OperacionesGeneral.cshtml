@functions{
    /**/

    public string ColoresSemaforo(string numColor)
    {
        string estilo = string.Empty;

        switch (numColor)

        {

            case "V":

                estilo = "rowVerde";

                break;

            case "A":

                estilo = "rowAmarillo";

                break;

            case "R":

                estilo = "rowRojo";

                break;

            default:

                estilo = "rowInfo";

                break;
        }
        return estilo;
    }
}

