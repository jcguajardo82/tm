        function ValidateMoney() {
            var amount = document.getElementById("in_dinero3").value;
            console.log(amount);
            //d+ permite caracteres enteros
            //si hay un caracter que no es dígito entonces evalua lo que está en paréntesis (?) significa opcional
            var patron = /^(\d+(.{1}\d{2})?)$/;
            if (!patron.test(amount)) {
                window.alert('cantidad ingresada incorrectamente');
                /*document.getElementById("in_dinero3").value = "";*/
                document.getElementById('in_dinero3').focus();
                return false;
            }
            else
                return true;
        }

