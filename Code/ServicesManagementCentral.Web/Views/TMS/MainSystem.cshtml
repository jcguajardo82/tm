        function Consulta(opc) {

            $("#btnIndCns1").removeClass("active");
            $("#btnIndCns2").removeClass("active");
            $("#btnIndCns3").removeClass("active");
            $("#btnIndCns4").removeClass("active");

            $('#btnIndCns' + opc).addClass('active');

