                            <thead>
                                <tr>
                                    <th>OrderNo</th>
                                    <th>UeNo</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>

                                @{

                                    if (Session["lisOrdenes"] != null)
                                    {

                                        var myDataSet = Session["lisOrdenes"] as System.Data.DataSet;
                                        foreach (System.Data.DataRow r in myDataSet.Tables[0].Rows)
                                        {
