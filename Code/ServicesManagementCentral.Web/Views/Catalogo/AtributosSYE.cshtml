                                <tr>
                                    <th>DescripcionServicio</th>
                                    <th>TipoEnvioRes</th>
                                    <th>AplicaCotizacion</th>
                                    <th>TarifaBase</th>
                                    <th>CCTarifaBase</th>
                                    <th>CargosExtra</th>
