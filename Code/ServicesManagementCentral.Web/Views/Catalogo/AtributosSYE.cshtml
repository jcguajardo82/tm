
@{
    ViewBag.Title = "Atributos de Seguro y Empaque";
}

<style>
    #radioBtn .notActive {
        color: #3276b1;
        background-color: #fff;
    }

    .red {
        background-color: red !important;
    }

    #table-scroll {
        height: 600px;
        overflow: auto;
        margin-top: 20px;
    }

    .modalPdfContainer {
        width: 100%;
        height: calc(100vh);
    }

    div.scroll_horizontal {
        width: 100%;
        overflow: auto;
    }
</style>


@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-4"></div>
        <div class="col-4"><h2> Atributos de seguro y empaque</h2></div>
        <div class="col-4"></div>
    </div>


    <div class="ibox-content p-xl" style="padding-top: 0px;padding-bottom: 0px;">
        <br />

        @*<table id="tblConsulta" class="table table-striped dataTable" style="width:100%">*@
       
            <div class="row" id="gridInvs">
                <br />
                <div class="col-12">

                    <table id="tblConsulta" class="table table-striped table-bordered table-hover" style="width: 100%">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Número de Categoria</th>
                                <th>Nombre de Categoria</th>
                                <th>Precio Factura de Proveedor a Soriana</th>
                                <th>Seguro Transportista</th>
                                @*<th>% Adicional de seguro para incluir en $ <br />*@
                                <th>(% vs costo de fact. del art. a nivel pza)</th>
                                <th>% adicional por empaque </th>
                                <th>Usuario de Actualizacion</th>
                                <th>Fecha de Actualizacion</th>
                                <th>Hora de Actualizacion</th>
                                <th>Activo</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>

                            @{

                                @*if (Session["listaASYE"] != null)
                                    {

                                        var myDataSet = Session["listaASYE"] as System.Data.DataSet;
                                        foreach (System.Data.DataRow r in myDataSet.Tables[0].Rows)
                                        {
                                            <tr>
                                                <td>@r["Id_AtributosSYE"]</td>
                                                <td>@r["Id_CategoriaArt"]</td>
                                                <td>@r["Id_CategoriaArt"]</td>
                                                <td>@r["Precio_Fact"]</td>
                                                <td>@r["Seguro_Trans"]</td>
                                                <td>@r["Porc_VS_Costo_Fact"]</td>
                                                <td>@r["Porc_adic_por_empaque"]</td>
                                                <td>@r["Usuario"]</td>
                                                <td>@r["Fec_Movto"]</td>
                                                <td>@r["Time_Movto"]</td>
                                                <td>@r["BitActivo"]</td>o
                                                <td>
                                                    <button type="button" onclick="SelectOrder('@r["Id_AtributosSYE"]','@r["Id_AtributosSYE"]')" class="btn btn-warning btn-circle"> <i class="fa fa-minus"></i></button>
                                                </td>
                                            </tr>

                                        }
                                    }*@


                                if (Session["grid"] != null)
                                {
                                    var myDataSet = Session["grid"] as List<ServicesManagement.Web.Controllers.AtributosModels>;
                                    foreach (ServicesManagement.Web.Controllers.AtributosModels r in myDataSet)
                                    {
                                        <tr>
                                            <td>@r.Id_AtributosSYE</td>
                                            <td>@r.Id_CategoriaArt</td>
                                            <td>@r.CategoriaArt</td>
                                            <td>@r.Precio_Fact</td>
                                            <td>@r.Seguro_Trans</td>
                                            <td>@r.Porc_VS_Costo_Fact</td>
                                            <td>@r.Porc_adic_por_empaque</td>
                                            <td>@r.Usuario</td>
                                            <td>@r.Fec_Movto</td>
                                            <td>@r.Time_Movto</td>
                                            @if (r.BitActivo == "1")
                                            {
                                                <td>Activo</td>
                                            }
                                            else
                                            {
                                                <td>Inactivo</td>
                                            }
