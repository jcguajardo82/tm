            googleMap.addListener("click", (mapsMouseEvent) => {
                // Close the current InfoWindow.
                infoWindow.close();

                infoWindow.setPosition(mapsMouseEvent.latLng);
                infoWindow.setContent(`Latitud: ${mapsMouseEvent.latLng.lat().toFixed(6)} Longitud: ${mapsMouseEvent.latLng.lng().toFixed(6)}`);
