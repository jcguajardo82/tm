            var bounds = new google.maps.LatLngBounds();
            var center = { lat: 19.181198, lng: -96.161272 };

            var options = {
                zoom: 18,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                center: center
            };

            var googleMap = new google.maps.Map($("#map")[0], options);
            console.log(googleMap);
            googleMap.setZoom(18);
            //var infoWindow = new google.maps.InfoWindow({ content: "Cargando..." });

            // Create the initial InfoWindow.
            let infoWindow = new google.maps.InfoWindow({
                content: "Da un click al mapa para leer Lat/Lng!",
                position: center,
            });
            infoWindow.open(googleMap);

            const marker = new google.maps.Marker({
                map: googleMap,
                draggable: true,
                position: center,
                title: 'Ubicación de Inicio'
            });

            const circle = new google.maps.Circle({
                map: googleMap,
                center: center,
                radius: 10000,
                fillColor: '#AA0000'
            });

            circle.bindTo('center', marker, 'position');
            marker.setVisible(false);
            circle.setVisible(false);



